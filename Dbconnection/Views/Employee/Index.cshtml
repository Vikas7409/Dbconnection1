@model IEnumerable<Dbconnection.Models.Employee>
@{
}
<a asp-action="Create" asp-controller="Employee" class="btn btn-outline-danger">create</a>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Mobile</th>
            <th>Address</th>
        </tr>
    </thead>
    @foreach(var emp in Model)
            {
                <tr>
                    <td>@emp.Id</td>
                    <td>@emp.Name</td>
                    <td>@emp.Email</td>
                    <td>@emp.Mobile</td>
                    <td>@emp.Address</td>
                </tr>
            }
</table>